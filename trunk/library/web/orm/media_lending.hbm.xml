<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.library.command.MediaLending" table="media_lending">
		<id name="mediaLendingId" column="media_lending_id">
			<generator class="increment">
			</generator>
		</id>
		<property name="lendingDate"  column="lending_datetime"/>	
		<property name="returnDate"  column="return_datetime"/>
		<property name="actualReturnDate"  column="actual_return_datetime"/>
		<many-to-one name="media" class="com.library.command.Media"
			lazy="false" update="false">
			<column name="media_id"/>
		</many-to-one>
		<many-to-one name="account" class="com.library.command.Account"
			lazy="false" update="false">
			<column name="account_id"/>
		</many-to-one>
		<property name="updatedBy"  column="updated_by"/>
		<property name="updatedDate"  column="updated_datetime"/>
	</class>

</hibernate-mapping>