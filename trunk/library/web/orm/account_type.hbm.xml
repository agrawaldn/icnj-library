<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.library.command.AccountType" table="account_type">
		<id name="accountTypeId" column="account_type_id">
			<generator class="increment">
			</generator>
		</id>
		<property name="accountType"  column="account_type"/>
		<property name="maxBorrowLimit"  column="max_borrow_limit_nb"/>	
		<property name="updatedBy"  column="updated_by"/>
		<property name="updatedDate"  column="updated_datetime"/>
	</class>

</hibernate-mapping>